<div class="card" style="width: 18rem;">
  <div class="card-body">
     {{#each items}}
    <h5 class="card-title">{{this.title}}</h5>
    <h6 class="card-subtitle mb-2 text-muted">Price  {{this.price}}</h6>
    <p class="card-text">{{this.description}}</p>
    {{/each}}
  </div>
</div>
<ul>
  {{#each items}}
    <li>{{this.title}}</li>
  {{/each}}
</ul>

